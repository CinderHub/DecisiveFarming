template(name="rightSidebar")
  .sidebarToggle.right.fixed(data-state="{{getSession 'rightSidebarToggleState'}}")
    .sidebarNotch
      .iconWrapper.right.size-24
        {{icon 'sidebar'}}
  .sidebar.right.fixed(data-state="{{getSession 'rightSidebarState'}}",data-sidebarToggleState="{{getSession 'rightSidebarToggleState'}}")
    {{#if currentParamIs 'section' 'field-info'}}
    .section
      .title Field ID
      form
        ul.list
          {{#if equalsSession 'fieldEditMode' 'edit'}}
          li.item.collapse
            label(style="width:70px;") Field Name
            input(style="width:110px;",type="text",placeholder="Field Name")
            .iconWrapper.right.size-24
              {{icon 'remove'}}
          li.item.collapse
            label(style="width:70px;") Division
            select.select
              option(value="Division") Division
              option(value="First") First
              option(value="Second") Second
              option(value="Third") Third
              option(value="Fourth") Fourth
              option(value="Fifth") Fifth
              option(value="Sixth") Sixth
              option(value="Seventh") Seventh
              option(value="Eighth") Eighth
              option(value="Ninth") Ninth
              option(value="Tenth") Tenth
          li.item.collapse
            label(style="width:70px;") Section
            input(style="width:70px;",type="text",placeholder="Section")
          li.item.collapse
            label(style="width:70px;") Township
            input(style="width:70px;",type="text",placeholder="Township")
          li.item.collapse
            label(style="width:70px;") Range
            input(style="width:70px;",type="text",placeholder="Range")
          li.item.collapse
            label(style="width:70px;") Meridian
            select.select
              option(value="Meridian") Meridian
          li.item.collapse
            label(style="width:70px;") Subdivision
            select.select
              option(value="Subdivision") Subdivision
          {{else}}
          li.item.collapse
            h3 Canola (Invigor)
            h3 175.2 acres
            h3 Projected Yield: 72 bu/acre
          {{/if}}
          li.item.collapse
            {{#if equalsSession 'fieldEditMode' 'edit'}}
            .button.rounded(style="margin-bottom:10px;",data-action="toggleNotice",data-notice="field-boundary")
              | Edit Field Boundary
            {{/if}}
            .button.rounded(data-action="toggleFieldDetails")
              {{#if equalsSession 'fieldEditMode' 'edit'}}
