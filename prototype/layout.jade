template(name="leftSidebar")
  .sidebar.firstPane.left(data-state="{{getSession 'leftSidebarState'}}",data-sidebarToggleState="{{getSession 'leftSidebarToggleState'}}")
    .secondPane
      .thirdPane
        ul.list
          {{#if currentContextIs 'fields'}}
          li.listItem
            a.listLink(data-action="addField",href="/farm-planner/crop-plans")
              .title Add New Field
              .iconWrapper.right.size-16
                {{icon 'new'}}
          li.listItem
            a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/370",data-action="changeField",data-field-id="370",data-active="{{equalsSession 'activeSidebarPath' '370'}}")
              .title Field One
              .subtitle E 24 28 26 W4
          li.listItem
            a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/371",data-action="changeField",data-field-id="371",data-active="{{equalsSession 'activeSidebarPath' '371'}}")
              .title Field Two
              .subtitle NE 21 28 25 W4
          {{/if}}

          {{#if currentContextIs 'equipment'}}
          li.listItem
            a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}",data-equipment-id="1337",data-action="addEquipment")
              .title Add New Equipment
              .iconWrapper.right.size-16
                {{icon 'new'}}
          li.listItem
            a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/1337",data-equipment-id="1337",data-active="{{equalsSession 'activeSidebarPath' '1337'}}")
              .title Big Red
              .subtitle Pattison PLS 3300
          {{/if}}

          {{#if currentContextIs 'market'}}
