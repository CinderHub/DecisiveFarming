              {{icon 'soilTesting'}}
            | Soil Testing
        li.navItem
          a.navLink(href="/{{currentContext}}/fertility-map/370",data-active="{{equalsSession 'activeSubheaderPath' 'fertility-map'}}",data-progress="incomplete")
            .iconWrapper.left.size-24
              {{icon 'fertilityMap'}}
            | Fertility Map
        li.navItem
          a.navLink(href="/{{currentContext}}/rx-rates/370",data-active="{{equalsSession 'activeSubheaderPath' 'rx-rates'}}",data-progress="{{#if currentParamIs 'assetsInfo' '370'}}inactive{{else}}incomplete{{/if}}")
