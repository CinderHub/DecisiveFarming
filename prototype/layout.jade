template(name="notice")
  .notice(data-hidden="{{getSession 'noticeHidden'}}")
    {{#if equalsSession 'notice' 'field-boundary'}}
    .title
      | Draw a basic field boundary
    ul.list.inline
      li.listItem
        .iconWrapper.left.size-16
          {{icon 'undo'}}
        | undo
      li.listItem
        .iconWrapper.left.size-16
          {{icon 'redo'}}
        | redo
      li.listItem
        .iconWrapper.left.size-16
          {{icon 'remove'}}
        | clear
      li.listItem(data-action="toggleNotice")
        .iconWrapper.left.size-16
          {{icon 'approve'}}
        | done
    {{/if}}

    {{#if equalsSession 'notice' 'confirm-boundary'}}
    .title Please confirm field boundary
    a.subtitle(href="#") more info
    ul.list.inline
      li.listItem(data-action="setNotice",data-notice="review-complete")
        .iconWrapper.left.size-16
          {{icon 'approve'}}
        | approve
      li.listItem(data-action="setNotice",data-notice="correct-boundary")
        .iconWrapper.left.size-16
          {{icon 'remove'}}
        | request changes
    {{/if}}

    {{#if equalsSession 'notice' 'correct-boundary'}}
    .title Click on the map to draw corrections
    ul.list.inline
      li.listItem
        .iconWrapper.left.size-16
          {{icon 'comment'}}
        | comment
      li.listItem(data-action="setNotice",data-notice="confirm-boundary")
        .iconWrapper.left.size-16
          {{icon 'remove'}}
        | cancel
      li.listItem(data-action="toggleNotice")
        .iconWrapper.left.size-16
          {{icon 'approve'}}
        | save
    {{/if}}

    {{#if equalsSession 'notice' 'review-complete'}}
    .title Review Complete!
      .iconWrapper.right.size-16(data-action="toggleNotice")
        {{icon 'remove'}}
    .message Any boundaries with requested changes will be updated shortly, thanks.
    {{/if}}

    {{#if equalsSession 'notice' 'verification-in-progress'}}
    .title Vertification of Boundary in Progress..
      .iconWrapper.right.size-16(data-action="toggleNotice")
        {{icon 'remove'}}
    .message Decisive Farming is preparing a detailed boundary for this field. We will notify you when it is ready for your review.
    {{/if}}


