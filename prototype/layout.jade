              td.td(colspan="2")
                .cell Total Acres
              td.td
                .cell 217.9
        {{/if}}

        {{#if currentParamIs 'section' 'rx-rates'}}
        .title Agronomist Notes
          .iconWrapper.right.size-16(data-action="toggleCover",data-cover="notes")
            {{icon 'new'}}
        ul.list
          li.listItem(data-action="toggleCover",data-cover="viewNote") Blend = 7.3-34-20-0
        .title JD 1910 - Tank Configuration
        form.form
          ul.list
            li.listItem
              .listLabel Tank 1
              input.listInput(style="width:90px;",type="text",placeholder="200 bushels",value="200 bushels")
              input.listInput(style="width:90px;",type="text",placeholder="Seed",value="Seed")
            li.listItem
              .listLabel Tank 2
              input.listInput(style="width:90px;",type="text",placeholder="150 bushels",value="150 bushels")
              input.listInput(style="width:90px;",type="text",placeholder="Blend",value="Blend")
            li.listItem
              .listLabel Tank 3
              input.listInput(style="width:90px;",type="text",placeholder="200 bushels",value="200 bushels")
              input.listInput(style="width:90px;",type="text",placeholder="Urea",value="Urea")
        .footer
          .button.rounded(href="") View a summary of all fields
        {{/if}}

        {{#if currentParamIs 'section' 'hedge-plan'}}
        .title Canola 2014 - Hedge
        .progress.rounded
          .meter(style="width: 55%")
          .subtitle 55% Hedged (398 of 725.25 MT)
        .title Time Triggers
        form.form
          ul.list
