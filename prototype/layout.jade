      li
        a(data-action="setNotice",data-notice="confirm-boundary")
          .iconWrapper.left.size-16
            {{icon 'remove'}}
          | cancel
      li
        a(data-action="toggleNotice")
          .iconWrapper.left.size-16
            {{icon 'approve'}}
          | save
    {{/if}}

    {{#if equalsSession 'notice' 'review-complete'}}
    .title Review Complete!
    .iconWrapper.right.size-16(data-action="toggleNotice")
      {{icon 'remove'}}
    .message Any boundaries with requested changes will be updated shortly, thanks.
    {{/if}}

    {{#if equalsSession 'notice' 'verification-in-progress'}}
    .title Vertification of Boundary in Progress..
    .iconWrapper.right.size-16(data-action="toggleNotice")
      {{icon 'remove'}}
    .message Decisive Farming is preparing a detailed boundary for this field. We will notify you when it is ready for your review.
    {{/if}}


