                  option(value="Option") Option
                  option(value="Future Cross-Hedge") Future Cross-Hedge
                  option(value="OTC Forward") OTC Forward
              {{/if}}
              {{#if equalsSession 'positionType' 'Future Cross-Hedge'}}
              li.listItem
                .listLabel Cross-hedge
                select.select
                  option(value="Select Commodity") Select Commodity
              {{/if}}
              li.listItem
                .listLabel Contract With
                input.listInput(type="text",placeholder="Name")
              li.listItem
                .listLabel Trade Date
                input.listInput.datepicker(type="text",placeholder="DD/MM/YYYY")
              li.listItem
                .listLabel Side
                select.select
                  option(value="Buy") Buy
                  option(value="Sell") Sell
              li.listItem
                .listLabel Contract Month
                input.listInput.datepicker(type="text",placeholder="DD/MM/YYYY")
              {{#if equalsSession 'positionType' 'Option'}}
              li.listItem
                .listLabel Option Type
                select.select
                  option(value="Put") Put
              li.listItem
                .listLabel Option Strike
                input.listInput(type="text",placeholder="Strike Name")
              {{/if}}
              li.listItem
                .listLabel Volume
                input.listInput.left(type="text",placeholder="0",style="width:30px;")
                select.select.left
                  option(value="Tonnes") Tonnes
                  option(value="MT") MT
                  option(value="Litres") Litres
              li.listItem
                .listLabel Price
                input.listInput(type="text",placeholder="$0")
              {{#if equalsSession 'positionCommodity' 'Canola'}}
                {{#if equalsSession 'positionType' 'OTC Forward'}}
                li.listItem
                  .listLabel Basis Price
                  input.listInput(type="text",placeholder="$0")
                {{/if}}
              {{/if}}
              li.listItem
                textarea.expanding(type="text",placeholder="Notes")
            .footer
              a.button.left(data-action="addPosition",data-action="toggleCover") Save
              a.button.right(data-action="addPosition") Save & New
          {{/if}}

          {{#if coverIs 'viewNote'}}
          .title View Note
            .iconWrapper.right.size-16(data-action="toggleCover")
              {{icon 'remove'}}
          form.form
            ul.list
              li.listItem
                input.listInput(type="text",placeholder="Title",value="Note Title")
              li.listItem
                textarea.expanding(type="text",placeholder="Content") Note Content
            .footer
              a.button.left(data-action="uploadFile") Upload File
              input(style="display:none;",type="file")
              a.button.right(type="submit",data-action="toggleCover") Update
          {{/if}}

