head
  title Decisive Farming
  meta(name="apple-mobile-web-app-capable",content="yes")
  meta(name="apple-mobile-web-app-status-bar-style",content="black-translucent")
  meta(name="viewport",content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no")


//- Main Yield Context for Iron Router
body

//-Main Layout Template
template(name="layout")
  {{yield "header"}}

  {{yield "subHeader"}}

  {{yield "leftSidebar"}}

  {{yield "rightSidebar"}}


  {{#if showFieldsOverview}}
  .stubImage(href="/fields/field-info/370",style="background-image:url('/stubImages/overview.png');")
  {{/if}}
  {{#if showFieldsInfo}}
  .stubImage(href="/fields/field-info/370",style="background-image:url('/stubImages/fieldInfo.png');")
  {{/if}}

  .firstPane
    .secondPane
      .thirdPane
        {{yield}}

  {{yield "toolbar"}}

  {{>cover}}

//-Header Template
template(name="header")
  .header
    ul.nav
      li.navItem
        a.navLink(href="/dashboard",data-active="{{equalsSession 'activeHeaderPath' 'dashboard'}}")
          .iconWrapper.left.size-16
            {{icon 'dashboard'}}
          | Dashboard
      li.navItem
        a.navLink(href="/fields",data-active="{{equalsSession 'activeHeaderPath' 'fields'}}")
          .iconWrapper.left.size-16
            {{icon 'fields'}}
          | Fields
      li.navItem
        a.navLink(href="/equipment",data-active="{{equalsSession 'activeHeaderPath' 'equipment'}}")
          .iconWrapper.left.size-16
            {{icon 'equipment'}}
          | Equipment
      li.navItem
        a.navLink(href="/market",data-active="{{equalsSession 'activeHeaderPath' 'market'}}")
          .iconWrapper.left.size-16
            {{icon 'market'}}
          | Market
      li.navItem
        a.navLink(href="/farm-planner",data-active="{{equalsSession 'activeHeaderPath' 'farm-planner'}}")
          .iconWrapper.left.size-16
            {{icon 'farmPlanner'}}
          | Farm Planner
    ul.nav
      li.navItem
        a.navLink(href="/profile",data-active="{{equalsSession 'activeHeaderPath' 'profile'}}")
          .iconWrapper.left.size-16
            {{icon 'profile'}}
          | John Carpenter
      //-li
        a(href="/help")
          .iconWrapper.left.size-16
            {{icon 'help'}}
          | Help

template(name="subHeader")
  .subHeader(data-state="{{getSession 'subHeaderState'}}")
    {{yield "leftSidebarToggle"}}
