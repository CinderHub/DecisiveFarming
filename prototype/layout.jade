            {{#if currentParamIs 'section' 'crop-plans'}}
            li.listItem
              a.listLink(data-action="addField",href="/farm-planner/crop-plans")
                .title Add New Field
                .iconWrapper.right.size-16
                  {{icon 'new'}}
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/370",data-action="changeField",data-field-id="370",data-active="{{equalsSession 'activeSidebarPath' '370'}}")
                .title Field One
                .subtitle E 24 28 26 W4
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/371",data-action="changeField",data-field-id="371",data-active="{{equalsSession 'activeSidebarPath' '371'}}")
                .title Field Two
                .subtitle NE 21 28 25 W4
            {{/if}}

            {{!Revenue and Expenses}}
            {{#if currentParamIs 'section' 'revenue-and-expenses'}}
            li.listItem
              a.listLink(data-action="addCommodity")
                .title Add New Commodity
                .iconWrapper.right.size-16
                  {{icon 'new'}}
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/1221",data-commodity-id="1221",data-active="{{equalsSession 'activeSidebarPath' '1221'}}")
                .title Canola
                .subtitle 800 Ac
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/1010",data-commodity-id="1010",data-active="{{equalsSession 'activeSidebarPath' '1010'}}")
                .title Carrots
                .subtitle 540 Ac
            {{/if}}

            {{!Market Plan}}
            {{#if currentParamIs 'section' 'market-plan'}}
            li.listItem
              a.listLink(data-action="addCommodity")
                .title Add New Commodity
                .iconWrapper.right.size-16
                  {{icon 'new'}}
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/1221",data-commodity-id="1221",data-active="{{equalsSession 'activeSidebarPath' '1221'}}")
                .title Canola
                .subtitle 800 Ac
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/1010",data-commodity-id="1010",data-active="{{equalsSession 'activeSidebarPath' '1010'}}")
                .title Carrots
                .subtitle 540 Ac
            {{/if}}

            {{!Work Orders}}
            {{#if currentParamIs 'section' 'work-orders'}}
            li.listItem
              a.listLink(data-action="addWorkOrder",href="/farm-planner/work-orders")
                .title Add New Work Order
                .iconWrapper.right.size-16
                  {{icon 'new'}}
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/370",data-active="{{equalsSession 'activeSidebarPath' '370'}}")
                .title Work Order One
                .subtitle E 24 28 26 W4
            li.listItem
              a.listLink(href="/{{currentContext}}/{{currentParam 'section'}}/371",data-active="{{equalsSession 'activeSidebarPath' '371'}}")
