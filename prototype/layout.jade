              {{icon 'rxRates'}}
            | RX Rates
        li.navItem
          a.navLink(href="/{{currentContext}}/rx-files/370",data-active="{{equalsSession 'activeSubheaderPath' 'rx-files'}}",data-progress="{{#if currentParamIs 'assetsInfo' '370'}}inactive{{else}}incomplete{{/if}}")
