          a.navLink(href="/{{currentContext}}/yield-report/370",data-active="{{equalsSession 'activeSubheaderPath' 'yield-report'}}")
            .iconWrapper.left.size-24
              {{icon 'yieldReport'}}
            | Yield Report
      {{/if}}

      {{#if currentContextIs 'market'}}
        li.navItem
          a.navLink(href="/{{currentContext}}/profit/{{getSession 'activeSidebarPath'}}",data-active="{{equalsSession 'activeSubheaderPath' 'profit'}}")
            .iconWrapper.left.size-24
              {{icon 'profit'}}
            | Profit
        li.navItem
          a.navLink(href="/{{currentContext}}/revenue-and-expenses/{{getSession 'activeSidebarPath'}}",data-active="{{equalsSession 'activeSubheaderPath' 'revenue-and-expenses'}}")
            .iconWrapper.left.size-24
              {{icon 'revenueAndExpenses'}}
            | Revenue & Expenses
        li.navItem
          a.navLink(href="/{{currentContext}}/hedge-plan/{{getSession 'activeSidebarPath'}}",data-active="{{equalsSession 'activeSubheaderPath' 'hedge-plan'}}")
            .iconWrapper.left.size-24
              {{icon 'hedgePlan'}}
            | Hedge Plan
        li.navItem
          a.navLink(href="/{{currentContext}}/positions/{{getSession 'activeSidebarPath'}}",data-active="{{equalsSession 'activeSubheaderPath' 'positions'}}")
            .iconWrapper.left.size-24
              {{icon 'positions'}}
            | Positions
        li.navItem
          a.navLink(href="/{{currentContext}}/research/{{getSession 'activeSidebarPath'}}",data-active="{{equalsSession 'activeSubheaderPath' 'research'}}")
            .iconWrapper.left.size-24
              {{icon 'research'}}
            | Research
      {{/if}}

      {{#if currentContextIs 'farm-planner'}}
        li.navItem
          a.navLink(href="/{{currentContext}}/crop-plans",data-active="{{equalsSession 'activeSubheaderPath' 'crop-plans'}}",data-progress="complete")
            .iconWrapper.left.size-24
              {{icon 'cropPlans'}}
            | Crop Plans
        //-li
          a.navLink(href="/{{currentContext}}/equipment",data-active="{{equalsSession 'activeSubheaderPath' 'equipment'}}")
            .iconWrapper.left.size-24
              {{icon 'equipment'}}
            | Equipment
        li.navItem
          a.navLink(href="/{{currentContext}}/revenue-and-expenses",data-active="{{equalsSession 'activeSubheaderPath' 'revenue-and-expenses'}}",data-progress="inProgress")
            .iconWrapper.left.size-24
              {{icon 'revenueAndExpenses'}}
            | Revenue And Expenses
        li.navItem
          a.navLink(href="/{{currentContext}}/market-plan/1221",data-active="{{equalsSession 'activeSubheaderPath' 'market-plan'}}",data-progress="incomplete")
            .iconWrapper.left.size-24
              {{icon 'marketPlan'}}
            | Market Plan
        li.navItem
          a.navLink(href="/{{currentContext}}/work-orders",data-active="{{equalsSession 'activeSubheaderPath' 'work-orders'}}",data-progress="incomplete")
            .iconWrapper.left.size-24
              {{icon 'workOrders'}}
            | Work Orders
      {{/if}}
    {{yield "rightSidebarToggle"}}

