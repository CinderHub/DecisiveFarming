          {{/if}}

template(name="rightSidebarToggle")
  .sidebarToggle.right(data-sidebarState="{{getSession 'rightSidebarToggleState'}}",data-state="{{getSession 'rightSidebarState'}}")
    .sidebarNotch
      .iconWrapper.right.size-24
        {{icon 'sidebar'}}

template(name="rightSidebar")
  .sidebar.firstPane.right(data-state="{{getSession 'rightSidebarState'}}",data-sidebarToggleState="{{getSession 'rightSidebarToggleState'}}")
    .secondPane
      .thirdPane
        {{#if currentParamIs 'section' 'field-info'}}
        .title Field ID
        form.form
          ul.list
            {{#if equalsSession 'fieldEditMode' 'edit'}}
            li.listItem
              .listLabel Field Name
              input.listInput(type="text",placeholder="Field Name")
              .iconWrapper.right.size-15
                {{icon 'remove'}}
            li.listItem
              .listLabel Division
              select.select
                option.option(value="Division") Division
                option.option(value="First") First
                option.option(value="Second") Second
                option.option(value="Third") Third
                option.option(value="Fourth") Fourth
                option.option(value="Fifth") Fifth
                option.option(value="Sixth") Sixth
                option.option(value="Seventh") Seventh
                option.option(value="Eighth") Eighth
                option.option(value="Ninth") Ninth
                option.option(value="Tenth") Tenth
            li.listItem
              .listLabel Section
              input.listInput(type="text",placeholder="Section")
            li.listItem
              .listLabel Township
              input.listInput(type="text",placeholder="Township")
            li.listItem
              .listLabel Range
              input.listInput(type="text",placeholder="Range")
            li.listItem
              .listLabel Meridian
              select.select
                option.option(value="Meridian") Meridian
            li.listItem
              .listLabel Subdivision
              select.select
                option.option(value="Subdivision") Subdivision
            {{else}}
            li.listItem
              .listLabel Canola (Invigor)
            li.listItem
              .listLabel 175.2 acres
            li.listItem
              .listLabel Projected Yield: 72 bu/acre
            {{/if}}
            li.listItem
              {{#if equalsSession 'fieldEditMode' 'edit'}}
              .button.rounded(style="margin-bottom:10px;",data-action="toggleNotice",data-notice="field-boundary")
                | Edit Field Boundary
              {{/if}}
              .button.rounded(data-action="toggleFieldDetails")
                {{#if equalsSession 'fieldEditMode' 'edit'}}
                | Save Field Details
                {{else}}
                | Edit Field Details
                {{/if}}
        .title Crop Plan
        form.form
          ul.list
            li.listItem
              .listLabel Acres
              input.listInput(type="text",placeholder="Acres",style="width:90px;margin-right:5px;")
              select.select
                option.option(value="Crop Type") Crop Type
            li.listItem
              .listLabel Variety
              input.listInput(type="text",placeholder="Variety")
            li.listItem
              .listLabel Yield
              input.listInput(type="text",placeholder="Yield",style="margin-right:5px;")
              select.select
                option.option(value="bu/ac") bu/ac
            li.listItem
              .listLabel Sure-Check Soil Testing
              input.listCheckbox(type="checkbox",name="sureCheck",value="no")
            li.listItem
              .listLabel Optimize RX
              input.listCheckbox(type="checkbox",name="optimizeRX",value="no")
      .title Notes
        a.iconWrapper.right.size-16(data-action="toggleCover",data-cover="notes")
          {{icon 'new'}}
      ul.list
        li.listItem
          a(data-action="toggleCover",data-cover="viewNote") 04 Oct 2012 - Harvest
        li.listItem
          a(data-action="toggleCover",data-cover="viewNote") 15 May 2013 - Seeding
        li.listItem
          a(data-action="toggleCover",data-cover="viewNote") 02 Jun 2013 - Scouting Notes
      .title Crop Rotation
      form.form
        ul.list
          li.listItem
            .listLabel 2015
            .subItem
              .listLabel Crop Type
              select.select
                option.option(value="Select") Select
                option.option(value="Canola") Canola
            .subItem
              .listLabel Yield
              input.listInput(style="width:28px;",type="text",placeholder="0")
              select.select
                option.option(value="bu/ac") bu/ac
                option.option(value="obs/ac") obs/ac
          li.listItem
            .listLabel 2013
            .subItem
              .listLabel Crop Type
              select.select
                option.option(value="Select") Select
                option.option(value="Canola") Canola
            .subItem
              .listLabel Yield
              input.listInput(style="width:28px;",type="text",placeholder="0")
              select.select
                option.option(value="bu/ac") bu/ac
                option.option(value="obs/ac") obs/ac
          li.listItem
            .listLabel 2012
            .subItem
              .listLabel Crop Type
              select.select
                option.option(value="Select") Select
                option.option(value="Canola") Canola
            .subItem
              .listLabel Yield
              input.listInput(style="width:28px;",type="text",placeholder="0")
              select.select
                option.option(value="bu/ac") bu/ac
                option.option(value="obs/ac") obs/ac
          li.listItem
            .listLabel 2011
            .subItem
              .listLabel Crop Type
              select.select
                option.option(value="Select") Select
                option.option(value="Canola") Canola
            .subItem
              .listLabel Yield
              input.listInput(style="width:28px;",type="text",placeholder="0")
              select.select
                option.option(value="bu/ac") bu/ac
                option.option(value="obs/ac") obs/ac
        .footer
          .button.rounded.left Cancel
          .button.rounded.right Save Field Details
        {{/if}}

        {{#if currentParamIs 'section' 'fertility-map'}}
        .title Field One
        ul.list
          li.listItem
            .listLabel Crop Plan
            .listLabel.normalWeight Invigor Canola
          li.listItem
            .listLabel Projected Yield
            .listLabel.normalWeight 72 bu/ac
        table.table
          thead.thead
            tr.tr
              td.td
                .cell Zone
              td.td
                .cell Fertility
                //- (bu/ac)
              td.td
                .cell Acres
          tbody.tbody
            tr.tr
              td.td(data-zone="1")
                .cell
              td.td
                .cell 66.411 - 81.332
              td.td
                .cell 6.0
            tr.tr
              td.td(data-zone="2")
                .cell
              td.td
                .cell 81.332 - 89.579
              td.td
                .cell 12.1
            tr.tr
              td.td(data-zone="3")
                .cell
              td.td
                .cell 89.597 - 95.151
              td.td
                .cell 17.8
            tr.tr
              td.td(data-zone="4")
                .cell
              td.td
